<mvc:View controllerName="logaligroup.sapui5.controller.App"
          xmlns="sap.m"
          xmlns:mvc="sap.ui.core.mvc"
          displayBlock="true">

    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page"
                    title="{i18n>appTitle}">
                    <content>
                        <mvc:XMLView id="_IDGenXMLView" viewName="logaligroup.sapui5.view.HelloPanel"></mvc:XMLView>
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>